<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from UserServiceService WSDL service -->

  <service name="UserService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service"/>
      <parameter name="wsdlServiceElement" value="UserServiceService"/>
      <parameter name="schemaQualified" value="http://service"/>
      <parameter name="wsdlServicePort" value="UserService"/>
      <parameter name="className" value="service.UserServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="UserService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="synchUsers" qname="operNS:synchUsers" xmlns:operNS="http://service" returnQName="retNS:synchUsersReturn" xmlns:retNS="http://service" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:action" xmlns:pns="http://service" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="synchUsers"/>

  </service>
</deployment>
